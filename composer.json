{
  "name": "savvywombat/laravel-assert-selector-contains",
  "description": "Targeted content assertions using CSS selector expressions",
  "type": "library",
  "homepage": "https://github.com/SavvyWombat/laravel-assert-selector-contains",
  "keywords": [
    "test",
    "savvywombat",
    "savvy wombat",
    "laravel",
    "test"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Stuart Jones",
      "email": "stuart@savvywombat.com.au"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-dom": "*",
    "ext-libxml": "*",
    "illuminate/support": "^9.29.0|^10.0",
    "symfony/css-selector": "^6.1.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.11.0",
    "orchestra/testbench": "^7.7.0|^8.0",
    "phpunit/phpunit": "^9.5.0|^10.0",
    "squizlabs/php_codesniffer": "^3.7.0"
  },
  "autoload": {
    "psr-4": {
      "SavvyWombat\\LaravelAssertSelectorContains\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "lint": "vendor/bin/php-cs-fixer fix . -v --dry-run --stop-on-violation --using-cache=no"
  }
}
